        "/me": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "User info",
                "description": "Returns User Data",
                "operationId": "InfoUser",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
